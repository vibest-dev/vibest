{
  "name": "ai-sdk-agents",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./claude-code": {
      "types": "./dist/claude-code/index.d.ts",
      "import": "./dist/claude-code/index.js",
      "require": "./dist/claude-code/index.cjs"
    }
  },
  "scripts": {
    "stub": "pnpm run build",
    "build": "tsdown --format cjs --no-dts --publint false && tsdown --format esm --dts --clean false",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "git clean -xdf node_modules .turbo dist"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.12",
    "@vibest/typescript": "workspace:*",
    "ai": "^5.0.63",
    "tsdown": "^0.13.5",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.22",
    "ai": "^5.0.63",
    "zod": "^3.24.1"
  }
}
