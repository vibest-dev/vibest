{
	"name": "ai-sdk-agents",
	"version": "0.0.1",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"stub": "tsdown",
		"build": "tsdown",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"clean": "git clean -xdf node_modules .turbo dist"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./claude-code": {
			"types": "./dist/claude-code/index.d.ts",
			"import": "./dist/claude-code/index.js",
			"require": "./dist/claude-code/index.cjs"
		}
	},
	"devDependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.2.19",
		"@vibest/typescript": "workspace:*",
		"ai": "^5.0.63",
		"zod": "^4.3.6",
		"tsdown": "^0.13.5"
	},
	"peerDependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.2.19",
		"ai": "^5.0.63",
		"zod": "^4.3.6"
	}
}
