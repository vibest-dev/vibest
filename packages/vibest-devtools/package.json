{
  "name": "vibest-devtools",
  "version": "0.0.4",
  "description": "A vite plugin that allow you direct chat with your claude code",
  "keywords": [
    "rspack",
    "unplugin",
    "vite",
    "webpack"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "exports": {
    "./vite": "./dist/vite.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "stub": "tsdown",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf node_modules dist .turbo"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.12",
    "@babel/core": "^7.28.3",
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@orpc/server": "^1.8.6",
    "@types/ws": "^8.18.1",
    "@vue/babel-plugin-jsx": "^1.5.0",
    "@vue/compiler-dom": "^3.5.19",
    "@vue/compiler-sfc": "^3.5.19",
    "ai": "^5.0.63",
    "magic-string": "^0.30.18",
    "pathe": "^2.0.3",
    "sirv": "^3.0.2",
    "tiny-invariant": "^1.3.3",
    "ws": "^8.18.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.10.13",
    "@vibest/agents": "workspace:*",
    "@vibest/code-inspector-node": "workspace:*",
    "@vibest/server-rpc": "workspace:*",
    "publint": "^0.3.12",
    "tsdown": "^0.13.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
